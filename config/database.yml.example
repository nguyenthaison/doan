default: &default
  adapter: mysql2
  encoding: utf8
  pool: 5
  username: root
  password:
  socket: /var/run/mysqld/mysqld.sock

development:
  <<: *default
  database: faq_development

test:
  <<: *default
  database: faq_test

production:
  <<: *default
  database: faq_production
  username: faq
  password: <%= ENV["FAQ_DATABASE_PASSWORD"] %>
